{
  "name": "better-terminal-logs",
  "displayName": "Better Terminal Logs",
  "description": "View terminal logs in a collapsible, organized webview panel with real-time updates",
  "version": "0.0.1",
  "icon": "https://github.com/DimaIvashchuk/better-terminal-logs-vscode-extension/blob/main/assets/logo.png",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "better-terminal-logs.showBetterLogs",
        "title": "Better Terminal Logs: Show Better Logs"
      }
    ],
    "keybindings": [
      {
        "command": "better-terminal-logs.showBetterLogs",
        "key": "cmd+shift+l",
        "mac": "cmd+shift+l",
        "win": "ctrl+shift+l",
        "linux": "ctrl+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/vsce": "^3.7.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
